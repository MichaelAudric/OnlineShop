{
  "version": 2,
  "builds": [
    {
      "src": "shopping-app/package.json",
      "use": "@vercel/react"
    },
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/shopping-app/build/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/app.js"
    }
  ]
}
